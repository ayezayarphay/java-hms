<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXTextField?>
<?import java.lang.String?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>


<VBox stylesheets="@color.css" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/9.0.1">
   <styleClass>
      <String fx:value="prefSizeStaffDN" />
      <String fx:value="bgWhite" />
   </styleClass>
   <children>
      <HBox alignment="TOP_RIGHT">
         <children>
            <JFXTextField />
         </children>
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </padding>
      </HBox>
      <TableView prefHeight="621.0" prefWidth="1200.0">
        <columns>
          <TableColumn prefWidth="75.0" text="Staff ID" />
          <TableColumn prefWidth="75.0" text="Patient ID" />
        </columns>
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
      </TableView>
   </children>
</VBox>
